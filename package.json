  "devDependencies": {
    "@babel/core": "^7.18.10",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "prettier": "2.8.7",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "webpack": "^5.74.0",
